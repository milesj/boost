{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "pretty": true,
    "removeComments": false,
    "sourceMap": false,
    "strict": true,
    "target": "es2018",
    "useDefineForClassFields": false,
    "jsx": "react",
    "composite": true,
    "declarationMap": true
  }
}