{
  "name": "@boost/plugin",
  "version": "4.0.0-alpha.1",
  "type": "commonjs",
  "release": "1651512175959",
  "description": "Plugin based architecture that supports module loading, custom types, registries, scopes, and more.",
  "keywords": [
    "boost",
    "plugin",
    "pluggable",
    "system",
    "architecture"
  ],
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/plugin"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/common": "^4.0.0-alpha.1",
    "@boost/debug": "^4.0.0-alpha.1",
    "@boost/event": "^4.0.0-alpha.1",
    "@boost/internal": "^4.0.0-alpha.1",
    "@boost/module": "^4.0.0-alpha.1",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@boost/test-utils": "^4.0.0-alpha.1",
    "@types/pluralize": "^0.0.29"
  },
  "packemon": {
    "api": "private",
    "format": "cjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
