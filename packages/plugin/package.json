{
  "name": "@boost/plugin",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "Plugin based architecture that supports module loading, custom types, registries, scopes, and more.",
  "keywords": [
    "boost",
    "plugin",
    "pluggable",
    "system",
    "architecture"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/plugin"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/common": "^3.0.0-alpha.0",
    "@boost/debug": "^3.0.0-alpha.0",
    "@boost/event": "^3.0.0-alpha.0",
    "@boost/internal": "^3.0.0-alpha.0",
    "@boost/module": "^3.0.0-alpha.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@boost/test-utils": "^3.0.0-alpha.0",
    "@jest/globals": "*",
    "@types/pluralize": "^0.0.29"
  },
  "packemon": {
    "format": "mjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    }
  }
}
