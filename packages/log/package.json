{
  "name": "@boost/log",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "Lightweight level based logging system.",
  "keywords": [
    "boost",
    "log",
    "logger",
    "logging",
    "levels"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "res/",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.mjs"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/log"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/common": "^3.0.0-alpha.0",
    "@boost/internal": "^3.0.0-alpha.0",
    "@boost/translate": "^3.0.0-alpha.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@jest/globals": "*"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "format": "mjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    },
    "./test": {
      "node": {
        "import": "./mjs/test.mjs"
      },
      "types": "./dts/test.d.ts"
    }
  }
}
