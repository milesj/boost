{
  "name": "@boost/common",
  "version": "4.0.0-alpha.1",
  "type": "commonjs",
  "release": "1651512176767",
  "description": "A collection of common utilities, classes, and helpers.",
  "keywords": [
    "boost",
    "common",
    "utils",
    "helpers"
  ],
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/common"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/decorators": "^4.0.0-alpha.1",
    "@boost/internal": "^4.0.0-alpha.1",
    "fast-glob": "^3.2.11",
    "json5": "^2.2.1",
    "optimal": "^5.1.1",
    "pretty-ms": "^7.0.1",
    "resolve": "^1.22.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@boost/test-utils": "^4.0.0-alpha.1",
    "@types/resolve": "^1.20.2"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "api": "private",
    "inputs": {
      "index": "src/index.ts",
      "optimal": "src/optimal.ts",
      "test": "src/test.ts"
    },
    "format": "cjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    "./optimal": {
      "types": "./dts/optimal.d.ts",
      "node": {
        "import": "./cjs/optimal-wrapper.mjs",
        "require": "./cjs/optimal.cjs"
      }
    },
    "./test": {
      "types": "./dts/test.d.ts",
      "node": {
        "import": "./cjs/test-wrapper.mjs",
        "require": "./cjs/test.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
