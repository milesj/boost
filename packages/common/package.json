  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/decorators": "^5.0.0-alpha.0",
    "@boost/internal": "^5.0.0-alpha.0",
    "enhanced-resolve": "^5.15.0",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "optimal": "^5.1.1",
    "pretty-ms": "^8.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@boost/test-utils": "^5.0.0-alpha.0"
  },
  "peerDependencies": {
    "typescript": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "api": "private",
    "inputs": {
      "index": "src/index.ts",
      "optimal": "src/optimal.ts",
      "test": "src/test.ts"
    },
    "format": "mjs",
    "platform": "node"
