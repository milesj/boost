{
  "name": "@boost/common",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "A collection of common utilities, classes, and helpers.",
  "keywords": [
    "boost",
    "common",
    "utils",
    "helpers"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.mjs"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/common"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/decorators": "^3.0.0-alpha.0",
    "@boost/internal": "^3.0.0-alpha.0",
    "fast-glob": "^3.2.7",
    "json5": "^2.2.0",
    "optimal": "^4.3.0",
    "pretty-ms": "^7.0.1",
    "resolve": "^1.20.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@boost/test-utils": "^3.0.0-alpha.0",
    "@jest/globals": "*",
    "@types/resolve": "^1.20.1"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "format": "mjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    },
    "./test": {
      "node": {
        "import": "./mjs/test.mjs"
      },
      "types": "./dts/test.d.ts"
    }
  }
}
