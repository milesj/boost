    "./test": {
      "node": {
        "types": "./mjs/test.d.ts",
        "import": "./mjs/test.mjs"
      }
    },
    ".": {
      "node": {
        "types": "./mjs/index.d.ts",
        "import": "./mjs/index.mjs"
      }
    }
  }
