{
  "name": "@boost/event",
  "version": "3.0.1",
  "type": "commonjs",
  "release": "1594765247526",
  "description": "An event system with multiple emitter patterns.",
  "keywords": [
    "boost",
    "event",
    "emitter",
    "type-safe"
  ],
  "main": "./cjs/index.cjs",
  "browser": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/event"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/internal": "^3.0.1"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": [
    {
      "api": "private",
      "format": "esm",
      "platform": "browser"
    },
    {
      "api": "private",
      "format": "cjs",
      "platform": "node"
    }
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/index.js"
      },
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
