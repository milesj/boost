{
  "name": "@boost/event",
  "version": "5.0.0-alpha.2",
  "type": "module",
  "release": "1651512177861",
  "description": "An event system with multiple emitter patterns.",
  "keywords": [
    "boost",
    "event",
    "emitter",
    "type-safe"
  ],
  "main": "./mjs/index.mjs",
  "module": "./esm/index.js",
  "types": "./mjs/index.d.ts",
  "files": [
    "esm/**/*",
    "mjs/**/*",
    "src/**/*"
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/event"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/internal": "^5.0.0-alpha.2"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": [
    {
      "api": "private",
      "format": "esm",
      "platform": "browser"
    },
    {
      "api": "private",
      "format": "mjs",
      "platform": "node"
    }
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "types": "./mjs/index.d.ts",
        "import": "./mjs/index.mjs"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      }
    }
  }
}
