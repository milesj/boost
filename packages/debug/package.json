{
  "name": "@boost/debug",
  "version": "4.0.0",
  "type": "commonjs",
  "release": "1651512175897",
  "description": "Lightweight debugging and crash reporting.",
  "keywords": [
    "boost",
    "debug",
    "debugger",
    "debugging",
    "crash",
    "report",
    "reporting"
  ],
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/debug"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/common": "^4.0.0",
    "@boost/internal": "^4.0.0",
    "@types/debug": "^4.1.7",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "api": "private",
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "format": "cjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    "./test": {
      "types": "./dts/test.d.ts",
      "node": {
        "import": "./cjs/test-wrapper.mjs",
        "require": "./cjs/test.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
