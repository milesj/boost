{
  "name": "@boost/debug",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "Lightweight debugging and crash reporting.",
  "keywords": [
    "boost",
    "debug",
    "debugger",
    "debugging",
    "crash",
    "report",
    "reporting"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.mjs"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/debug"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/common": "^3.0.0-alpha.0",
    "@boost/internal": "^3.0.0-alpha.0",
    "@types/debug": "^4.1.7",
    "debug": "^4.3.2",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.7"
  },
  "devDependencies": {
    "@jest/globals": "*"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "format": "mjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    },
    "./test": {
      "node": {
        "import": "./mjs/test.mjs"
      },
      "types": "./dts/test.d.ts"
    }
  }
}
