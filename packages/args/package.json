{
  "name": "@boost/args",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "A convention based argument parsing and formatting library, with strict validation checks.",
  "keywords": [
    "boost",
    "argv",
    "args",
    "arguments",
    "options",
    "flags",
    "params",
    "cli"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/args"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/internal": "^3.0.0-alpha.0",
    "levenary": "^1.1.1"
  },
  "devDependencies": {
    "@jest/globals": "*"
  },
  "packemon": {
    "format": "mjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    }
  }
}
