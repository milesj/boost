{
  "name": "@boost/args",
  "version": "4.0.1",
  "type": "commonjs",
  "release": "1651512176134",
  "description": "A convention based argument parsing and formatting library, with strict validation checks.",
  "keywords": [
    "boost",
    "argv",
    "args",
    "arguments",
    "options",
    "flags",
    "params",
    "cli"
  ],
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/args"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/internal": "^4.0.1",
    "levenary": "^1.1.1"
  },
  "packemon": {
    "api": "private",
    "format": "cjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
