{
  "name": "@boost/cli",
  "version": "3.0.0-alpha.0",
  "release": "1594765247526",
  "description": "An interactive command line program builder, powered by React and Ink.",
  "keywords": [
    "boost",
    "args",
    "cli",
    "command",
    "line",
    "interface",
    "program",
    "react",
    "ink"
  ],
  "type": "module",
  "module": "./mjs/index.mjs",
  "main": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "react.d.ts",
    "react.mjs",
    "res/",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.mjs"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/cli"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/args": "^3.0.0-alpha.0",
    "@boost/common": "^3.0.0-alpha.0",
    "@boost/event": "^3.0.0-alpha.0",
    "@boost/internal": "^3.0.0-alpha.0",
    "@boost/log": "^3.0.0-alpha.0",
    "@boost/terminal": "^3.0.0-alpha.0",
    "@boost/translate": "^3.0.0-alpha.0",
    "execa": "^5.1.1",
    "levenary": "^1.1.1",
    "semver": "^7.3.5"
  },
  "peerDependencies": {
    "ink": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "debug": "^4.3.2",
    "ink": "^3.0.9",
    "ink-testing-library": "^2.1.0",
    "react": "^17.0.2"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "react": "src/react.ts",
      "test": "src/test.ts"
    },
    "format": "mjs",
    "platform": "node"
  },
  "tsconfig": {
    "compilerOptions": {
      "lib": ["dom"],
      "jsx": "react"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./mjs/index.mjs"
      },
      "types": "./dts/index.d.ts"
    },
    "./test": {
      "node": {
        "import": "./mjs/test.mjs"
      },
      "types": "./dts/test.d.ts"
    },
    "./react": {
      "node": {
        "import": "./mjs/react.mjs"
      },
      "types": "./dts/react.d.ts"
    }
  }
}
