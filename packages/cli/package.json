{
  "name": "@boost/cli",
  "version": "4.0.0-alpha.0",
  "type": "commonjs",
  "release": "1651512177598",
  "description": "An interactive command line program builder, powered by React and Ink.",
  "keywords": [
    "boost",
    "args",
    "cli",
    "command",
    "line",
    "interface",
    "program",
    "react",
    "ink"
  ],
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "res/",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/boost.git",
    "directory": "packages/cli"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@boost/args": "^4.0.0-alpha.0",
    "@boost/common": "^4.0.0-alpha.0",
    "@boost/event": "^4.0.0-alpha.0",
    "@boost/internal": "^4.0.0-alpha.0",
    "@boost/log": "^4.0.0-alpha.0",
    "@boost/terminal": "^4.0.0-alpha.0",
    "@boost/translate": "^4.0.0-alpha.0",
    "execa": "^5.1.1",
    "levenary": "^1.1.1",
    "semver": "^7.3.7"
  },
  "peerDependencies": {
    "ink": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "debug": "^4.3.4",
    "ink": "^3.2.0",
    "ink-testing-library": "^2.1.0",
    "react": "^17.0.2"
  },
  "packemon": {
    "api": "private",
    "inputs": {
      "index": "src/index.ts",
      "react": "src/react.ts",
      "test": "src/test.ts"
    },
    "format": "cjs",
    "platform": "node"
  },
  "exports": {
    "./package.json": "./package.json",
    "./react": {
      "types": "./dts/react.d.ts",
      "node": {
        "import": "./cjs/react-wrapper.mjs",
        "require": "./cjs/react.cjs"
      }
    },
    "./test": {
      "types": "./dts/test.d.ts",
      "node": {
        "import": "./cjs/test-wrapper.mjs",
        "require": "./cjs/test.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
